<prism:PrismApplication x:Class="MarketDataViewer.Shell.App"
                        xmlns:prism="http://prismlibrary.com/"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:views="clr-namespace:MarketDataViewer.Controls.Views;assembly=MarketDataViewer.Controls"
                        xmlns:viewModels="clr-namespace:MarketDataViewer.Controls.ViewModels;assembly=MarketDataViewer.Controls">
    <prism:PrismApplication.Resources>
        <DataTemplate DataType="{x:Type viewModels:AddSymbolViewModel}">
            <views:AddSymbolView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:StockPricesViewModel}">
            <views:StockPricesView />
        </DataTemplate>
    </prism:PrismApplication.Resources>
</prism:PrismApplication>
